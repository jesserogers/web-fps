  "description": "Template for full stack Node.js apps in TypeScript",
  "main": "index.js",
  "scripts": {
    "_buildUI": "ng build --prod",
    "build:common": "tsc --p src/common",
    "build:engine": "tsc --p src/dissonance",
    "build:server": "tsc --p src/server",
    "build:client": "ng build --prod",
    "build:all": "npm run build:common && npm run build:server && npm run build:client",
    "server": "node dist/server/index.js",
    "client": "ng s"
